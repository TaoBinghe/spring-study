    <typeAliases>
        <package name="com.coder.domain"/>
    </typeAliases>
    
    <!-- 映射文件位置（可选，也可以在 Spring 配置中指定） -->
    <!-- <mappers>
        <mapper resource="com/coder/dao/AccountDao.xml"/>
    </mappers> -->
</configuration>
